//smallest in arr

public class dsa84 {
  public static void main(String args[])
  {
    int arr[]={5,15,22,1,-15,24};
    int small=Integer.MAX_VALUE;
    for(int i=0;i<arr.length;i++)
    {
      if(arr[i]<small)
      {
        small=arr[i];
      }
    }
    System.out.println("smallest is :" + small);
   
  }
  
}
